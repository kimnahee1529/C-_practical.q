//2047.newspaper_headlines
#include<iostream>
#include <string>
#include <sstream>
#include <fstream>
using namespace std;

int main(int argc, char** argv){
    string str;
    cin>>str;
        for(int i=0; i<str.size(); i++){
        	str[i]=toupper(str[i]);
        }
        cout<<str<<endl;

	return 0;
}
